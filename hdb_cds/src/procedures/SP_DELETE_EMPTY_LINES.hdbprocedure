PROCEDURE "sap_xsa_sandbox.hdb_cds.procedures::SP_DELETE_EMPTY_LINES"(
	-- IN IN_INT INTEGER
	IN FILENAME STRING
)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   AS
BEGIN
	delete from "sap_xsa_sandbox.hdb_cds::CongressMarks.LINES" where "FILENAME" = FILENAME AND ltrim(rtrim("TEXT")) = '';
END