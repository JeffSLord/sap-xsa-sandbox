{
	"welcomeFile": "webapp/index.html",
	"authenticationMethod": "route",
	"routes": [{
		"source": "/node/(.*)",
		"destination": "node-be",
		"target": "$1",
		"authenticationType": "xsuaa",
		"csrfProtection": true
	}, {
		"source": "(.*).xsodata(.*)",
		"destination": "nodexsjs_be",
		"authenticationType": "xsuaa",
		"scope": "$XSAPPNAME.Read"
	}]
}